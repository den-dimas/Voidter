        </ui:GroupBox>
        <ui:GroupBox name="Enemies" style="flex-direction: row; margin-top: 0; margin-right: 0; margin-bottom: 0; margin-left: 0; color: rgb(255, 255, 255); -unity-font-style: bold;">
            <ui:Label tabindex="-1" text="Enemies Left:" parse-escape-sequences="true" display-tooltip-when-elided="true" name="EnemiesText" style="font-size: 30%;" />
            <ui:Label tabindex="-1" text="100" parse-escape-sequences="true" display-tooltip-when-elided="true" name="EnemiesNumber" style="font-size: 30%;" />
        </ui:GroupBox>
    </ui:GroupBox>
